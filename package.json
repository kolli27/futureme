{
  "name": "futureme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "docker:build": "docker build -t futureme .",
    "docker:run": "docker run -p 3000:3000 futureme",
    "prod:build": "npm run type-check && npm run lint && npm run build",
    "db:migrate": "tsx src/lib/db/setup.ts migrate",
    "db:init": "tsx src/lib/db/setup.ts init",
    "db:health": "tsx src/lib/db/setup.ts health",
    "db:reset": "tsx src/lib/db/setup.ts reset",
    "seeds:run": "tsx src/lib/db/seeds/seedRunner.ts run",
    "seeds:setup": "tsx src/lib/db/seeds/seedRunner.ts setup",
    "seeds:clear": "tsx src/lib/db/seeds/seedRunner.ts clear",
    "seeds:check": "tsx src/lib/db/seeds/seedRunner.ts check",
    "seeds:perf": "tsx src/lib/db/seeds/seedRunner.ts perf"
  },
  "dependencies": {
    "@auth/pg-adapter": "^1.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@next/font": "^14.2.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/stripe-js": "^7.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.17",
    "argon2": "^0.43.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "express-rate-limit": "^8.0.1",
    "framer-motion": "^12.23.7",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "limiter": "^3.0.0",
    "lucide-react": "^0.525.0",
    "nanoid": "^5.1.5",
    "next": "14.1.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "pg": "^8.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.61.1",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.8",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.30",
    "jsdom": "^26.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.1",
    "typescript": "^5"
  }
}
